{
  "name": "@livingdata/tabular-data-helpers",
  "version": "0.0.0",
  "type": "module",
  "main": "lib/index.js",
  "browser": "browser/index.js",
  "repository": "https://github.com/livingdata-co/tabular-data-helpers",
  "author": "Jérôme Desboeufs <jerome@livingdata.co>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "browser"
  ],
  "scripts": {
    "lint": "xo",
    "test": "ava",
    "transpile": "swc lib/ -d browser/",
    "prepare": "npm run transpile"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.249",
    "ava": "^4.3.3",
    "get-stream": "^6.0.1",
    "into-stream": "^7.0.0",
    "xo": "^0.52.3"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "unicorn/text-encoding-identifier-case": "off",
      "unicorn/prefer-node-protocol": "off"
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "chardet": "^1.4.0",
    "events": "^3.3.0",
    "iconv-lite": "^0.6.3",
    "papaparse": "^5.3.2",
    "readable-stream": "^4.1.0"
  }
}
